<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.simplyvaldo.mylogins">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".profiles"
            android:label="Profiles">
        </activity>

        <activity
            android:name=".logins"
            android:label="Logins" >
        </activity>

        <activity
            android:name=".settings"
            android:label="Settings">
        </activity>

        <activity
            android:name=".createProfile"
            android:label="Create Profile">
        </activity>

        <activity
            android:name=".loginType"
            android:label="Login Type">
        </activity>

        <activity
            android:name=".showInfoLogin"
            android:label="Login Info">
        </activity>

        <activity
            android:name=".viewProfile"
            android:label="View Profile">
        </activity>

    </application>

</manifest>